  <script>

    /* Getting the Data for the dasboard counters  */
    $.getJSON('/getcounters?email=<%=email%>', function (data) {

      $('#registered-doctor').text(data["doctor"].length)
      $('#total-products').text(data["product"].length)
      $('#own-register').text(data["yourProduct"].length)
      $('#pending-approval').text(data['pending'].length);
      $('#approved-approval').text(data['approve'].length);
      $('#declined-approval').text(data['decline'].length);

    })
  </script>
