  <main style="margin-top: 58px">
    <div class="container pt-4">
      <section class="mb-4">
        <div class="card">
            <div class="card-header text-center py-3">
              <h5 class="mb-0 text-center">
                ADD DCR
              </h5>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-hover text-nowrap">
                    <form action="/admin/dcrform" method="post" id = "mainform">

                      <!-- User Email -->
                      <div class="col-md-12 mb-4">
                        <select class="select form-control" id="useremail" name="useremail" required >
                            <option value disabled selected>Select User Email</option>
                            
                        </select>
                       </div>

                        <div class="row">

                          <!-- Date -->
                          <div class="col-md-6 mb-4">
                            Date
                              <div class="form-outline textBox">
                                <input type="date" required id="date" name="date"  class="form-control form-control-lg"/ onchange="checksch()">
                              </div>
                          </div>
                          
                          <!-- Schedule Satus  -->
                          <div class="col-md-6 mb-4">
                            <div class="form-outline">
                                Schedule Status
                                <input type="text" required id="schedulestatus" name="schedulestatus" class="form-control form-control-lg"/ readonly>
                                <!-- <label class="form-label" for="productname">Product Name</label> -->
                            </div>
                          </div>

                          
                      </div>

                      <div class="row schdepend">

                        <!-- Visits  -->
                        <div class="col-md-6 mb-4">
                          Visits
                          <div class="form-outline textBox">
                            <input type="text" required id="visitOpt" name="visitOpt"
                                class="form-control form-control-lg" / readonly>
  
                        </div>
                        </div> 

                        <!-- Schedule Status  -->
                        <div class="col-md-6 mb-4">
                          Select Status
                          <select class="select form-control" id="status" name="status" required onchange="updateSchStatus(this)">
                              <option value="" disabled >Select Status</option>
                              <option value="Working">Working</option>
                              <option value="Work From Home">Work From Home</option>
                              <option value="Meeting">Meeting</option>
                              <option value="Holiday">Holiday</option>
                              <option value="Leave">Leave</option>
                              <option value="Strike">Strike</option>
                              <option value="Interview">Interview</option>
                              <option value="Training">Training</option>
                              <option value="Induction">Induction</option>
                              <option value="Transit">Transit</option>
                              <option value="Others">Others</option> 
                          </select>
                        </div> 
                      </div>
                    </form>
                </table>
              </div>
            </div>
          </div>
      </section>
      
    </div>
